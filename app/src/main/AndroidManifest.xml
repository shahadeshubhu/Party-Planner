<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sjsu.partyplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PartyPlanner">
        <activity android:name=".Activities.Parties.TaskListActivity"></activity>
        <activity android:name=".Activities.Parties.CreatePartyActivity" />
        <activity android:name=".Activities.Parties.PartyActivity" />
        <activity android:name=".Activities.Dashboard.DashboardActivity" />
        <activity android:name=".Activities.Dashboard.DetailFragment" />
        <activity android:name=".Activities.Dashboard.DetailActivity" />
        <activity android:name=".Activities.Users.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Users.RegistrationActivity">
            <intent-filter>

                <!-- <action android:name="android.intent.action.MAIN" /> -->

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>